#include<stdio.h>
int main()
{
  char c;
  printf("Enter a character: ");
  scanf("%c",&c);
  if((c>='A' && c<='Z')||(c>='a')&&(c<='z'))
  {
  if((c=='a')||(c=='e')||(c=='i')||(c=='o')||(c=='u')||(c=='A')||(c=='E')||(c=='I')||(c=='O')||(c=='U'))
  {
    printf("The character is a vowel");
  }
  else{
    printf("This is a consonant");
  }
  }
}
