#include<stdio.h>
int main() {
  int size;
  printf("Enter the no of elements : ");
  scanf("%d", &size);
  int array[size];
  for(int i = 0; i < size; i++) {
    scanf("%d", &array[i]);
  }
  int sum=0;
  for(int i = 0; i < size; i++)
    {
      sum+=array[i];
    }
  printf("Sum Of Array : %d",sum);
  return 0;
}
