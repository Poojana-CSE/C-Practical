#include<stdio.h>
int main() {
  int size;
  printf("Enter the no of elements : ");
  scanf("%d", &size);
  int array[size];
  for(int i = 0; i < size; i++) {
    scanf("%d", &array[i]);
  }
  for(int i=0;i<size;i++)
  {
    printf("%d ",array[i]);
  }
  printf("\n");
  int arraycpy[size];
  for(int i = 0; i < size; i++)
    {
      arraycpy[i]=array[i];
    }
  for(int i=0;i<size;i++)
    {
      printf("%d ",arraycpy[i]);
    }
  return 0;
}
