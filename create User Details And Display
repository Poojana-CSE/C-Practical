#include <stdio.h>
int main() {
  char name[100];
  int age;
  char gender[7];
  printf("Enter your name: ");
  scanf("%s", name);
  printf("Enter your age: ");
  scanf("%d", &age);
  printf("Enter your gender: ");
  scanf("%s", &gender);
  printf("Your name is %s\n", name);
  printf("Your age is %d\n", age);
  printf("Your gender is %s\n", gender);
  return 0;
}
